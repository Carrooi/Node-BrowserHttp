{
	"packages": {
		"tests": {
			"base": "../",
			"application": "./test/tests.js",
			"modules": [
				"./src/*.coffee",
				"./test/*.coffee",
				"./node_modules/q/q.js"
			],
			"coreModules": ["events", "util"],
			"aliases": {
				"browser-http": "src/Http",
				"browser-http/Helpers": "src/Helpers",
				"browser-http/Request": "src/Request",
				"browser-http/Response": "src/Response",
				"browser-http/Extensions/Forms": "src/Extensions/Forms",
				"browser-http/Extensions/Links": "src/Extensions/Links",
				"browser-http/Extensions/Loading": "src/Extensions/Loading",
				"browser-http/Extensions/Redirect": "src/Extensions/Redirect",
				"browser-http/Extensions/Snippets": "src/Extensions/Snippets"
			},
			"run": [
				"test/Helpers",
				"test/Http",
				"test/Extensions",
				"test/Queue"
			]
		}
	},
	"debugger": {
		"scripts": true
	}
}
