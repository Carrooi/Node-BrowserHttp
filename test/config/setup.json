{
	"packages": {
		"tests": {
			"base": "../",
			"target": "./test/application.js",
			"modules": [
				"./lib/*.js",
				"./test/tests/*.coffee",
				"q",
				"events"
			],
			"aliases": {
				"browser-http": "/lib/Http",
				"browser-http/Helpers": "/lib/Helpers",
				"browser-http/Request": "/lib/Request",
				"browser-http/Response": "/lib/Response",
				"browser-http/Extensions/Forms": "/lib/Extensions/Forms",
				"browser-http/Extensions/Links": "/lib/Extensions/Links",
				"browser-http/Extensions/Loading": "/lib/Extensions/Loading",
				"browser-http/Extensions/Redirect": "/lib/Extensions/Redirect",
				"browser-http/Extensions/Snippets": "/lib/Extensions/Snippets"
			},
			"run": [
				"/test/tests/Helpers",
				"/test/tests/Http",
				"/test/tests/Extensions",
				"/test/tests/Queue"
			]
		}
	}
}
