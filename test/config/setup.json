{
	"packages": {
		"tests": {
			"target": "./application.js",
			"modules": [
				"./tests/*.coffee"
			],
			"run": [
				"/tests/Helpers",
				"/tests/Http",
				"/tests/Extensions",
				"/tests/Queue"
			],
			"ignore": {
				"packageFiles": false
			}
		}
	},
	"debugger": {
		"minify": false,
		"filesStats": false
	}
}
